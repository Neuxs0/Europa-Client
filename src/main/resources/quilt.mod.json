{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "A simple client by Neuxs",
      "contributors": {
        "Neuxs": "Owner"
      },

      "contact": {
        "homepage": "https://neuxs.dev/",
        "issues": "",
        "sources": ""
      },

      "icon": "assets/europa_client/icon.png"
    },

    "entrypoints": {
      "init": "dev.neuxs.europa_client.Client"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": "*"
      },
      {
        "id": "cosmicreach",
        "versions": "*"
      }
    ]
  },

  "mixin": "europa_client.mixins.json"
}
